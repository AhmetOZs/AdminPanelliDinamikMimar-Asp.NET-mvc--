@using Mimar.Models.Siniflar
@model List<Anasayfa>
 
@{
    Layout = null;
    List<Yorum> yorums = ViewBag.yorum ?? new List<Yorum>();
    List<Iletisim> iletisims = ViewBag.iletisim ?? new List<Iletisim>();
    List<Referanslar> referanslar = ViewBag.referans ?? new List<Referanslar>();
    List<HizmetKategori> hizmetKategoris = ViewBag.kategori ?? new List<HizmetKategori>();
}


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <title>CBN Dekorasyon</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/images/favicon.png" />
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/css/flaticon.css" />
    <link rel="stylesheet" href="~/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="~/css/owl.theme.css" />
    <link rel="stylesheet" href="~/css/vegas.css" />
    <link rel="stylesheet" href="~/css/magnific-popup.css" />
    <link rel="stylesheet" href="~/css/lightgallery.css" />
    <link rel="stylesheet" href="~/css/woocommerce.css" />
    <link rel="stylesheet" href="~/css/royal-preload.css" />
    <link rel="stylesheet" href="~/css/editor-style.css" />
    <link rel="stylesheet" href="~/style.css" />
    <!-- REVOLUTION SLIDER CSS -->
</head>

<body class="header-vertical dark-scheme">
   
    <div id="royal_preloader"></div>
    <div id="page" class="site">
        <header id="site-header" class="site-header-vertical header-transparent">
            <!-- Main Header start -->
            <div class="header-desktop" style="background-image: url(images/bg/bg1.jpg); background-size:cover; ">
                <div class="side-nav">
                    <div class="side-nav-logo text-center sidenav-col">
                        <div id="site-nav-logo" class="site-logo">
                            <a href="@Url.Action("Index","Anasayfa")">
                                <img src="~/images/logo-dark.svg" alt="CBN" class="">
                            </a>
                        </div>
                    </div>
                    <div class="side-nav-main sidenav-col">
                        <nav style="top:200px" id="menu-left" class="vertical-main-navigation">
                            <ul class="menu">
                                <li class="current-menu-item"><a href="@Url.Action("Index","Main")">Anasayfa</a></li>
                                <li><a href="#about-side">Hakkımızda</a></li>
                                <li><a href="@Url.Action("Services","Anasayfa")">Hizmetlerimiz</a></li>
                                <li><a href="#portfolio-side">Referanslar</a></li>
                                <li><a href="@Url.Action("Iletisim","Anasayfa")">İletişim</a></li>
                            </ul>
                            
                        </nav>
                    </div>
                    
                    @foreach (var item in iletisims)
                    {
                        
                                                
                        <div style="position:relative; left:85px; top:300px" class="side-nav-social list-social">
                            <ul>
                                <li><a href="@item.Twitter" target="_self"><i class="fab fa-twitter"></i></a></li>
                                <li><a href="@item.Facebook" target="_self"><i class="fab fa-facebook-f"></i></a></li>
                                <li><a href="@item.Linkedin" target="_self"><i class="fab fa-linkedin-in"></i></a></li>
                                <li><a href="@item.Instagram" target="_self"><i class="fab fa-instagram"></i></a></li>
                                
                            </ul>
                            
                        </div>   
                        
                    
                    }
                    <div>
                        
                    </div>
                </div>
            </div>
            <div class="header_mobile">
                <div class="container-fluid">
                    <div class="octf-mainbar-row octf-row">
                        <div class="octf-col">
                            <div class="mlogo_wrapper clearfix">
                                <div class="mobile_logo">
                                    <a href="@Url.Action("Index","Anasayfa")">
                                        <img src="~/images/logo.svg" alt="CBN">
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="octf-col justify-content-end">
                            
                            <div class="octf-menu-mobile octf-cta-header">
                                <div id="mmenu-toggle" class="mmenu-toggle">
                                    <button><i class="ot-flaticon-menu"></i></button>
                                </div>
                                <div class="site-overlay mmenu-overlay"></div>
                                <div id="mmenu-wrapper" class="mmenu-wrapper on-right">
                                    <div class="mmenu-inner">
                                        <a class="mmenu-close" href="#"><i class="ot-flaticon-right-arrow"></i></a>
                                        <div class="mobile-nav">
                                            <ul id="menu-main-menu" class="mobile_mainmenu none-style">
                                                <li class="current-menu-item"><a href="@Url.Action("Index","AnaSayfa")">Ana Sayfa</a></li>
                                                <li><a href="#about-side">Hakkımızda</a></li>
                                                <li><a href="@Url.Action("Services","Anasayfa")">Hizmetlerimiz</a></li>                                              
                                               
                                                <li><a href="#portfolio-side">Referanslar</a></li>
                                                <li><a href="@Url.Action("Iletisim","Anasayfa")">İletişim</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
     
            
       
        <div id="content" class="site-content" >
            @foreach (var k in Model)
        {
            <div id="hero-section" data-number="2" data-image-1="images/slider/slideshow1-home6.jpg" data-image-2="images/slider/slideshow2-home6.jpg" data-effect="fade" style="background-image:url(@k.BüyükResim); background-size:1563px 969px;">
                <div class="hero-content text-center">
                    <img src="images/logo-home6.png" class="hero-logo" alt="">
                    <h2>@k.BüyükResimBaslik
                        
   
                    </h2>
                    <div class="ot-button">
                        <a href="#portfolio-side" class="octf-btn octf-btn-light">PROJELERİMİZE GÖZ ATIN</a>
                    </div>
                    <div class="hero-desc">
                        <p>@k.BüyükResimSolYazi</p>
                    </div>
                </div><!-- /.hero-content -->
                <div class="btn-scroll">
                    <a class="scroll-target scroll-down" href="#about-side">Aşağı İn</a>
                </div>
            </div>
               }
            <section id="about-side" class="about-side pb-0">
                <div class="container-fluid">
                    <div class="row">
                               @foreach (var a in Model)
                        {
                        <div class="col-xl-6 col-lg-12 text-center align-self-center mb-5 mb-xl-0">
                     
                            <div class="about-side-img">
                                <img src="@a.HakkimdaResim" alt="">
                            </div>
                        </div>
                   
                            
                        
                        <div class="offset-xl-0 col-xl-6 offset-lg-0 col-lg-12 col-md-8 offset-md-2">
                            <div class="ml-xl-35 mr-xxl">
                               
                                    
                                
                                    <span>[ @a.Hakkimdaparantezbaslik ]</span>
                                    <h2 class="main-heading">@a.HakkimdaBaslik1</h2>
                                
                                <div class="ot-accordions s-dark">
                                    <div class="acc-item">
                                        
                                        <p>
                                            &emsp;@a.HakkimdaAciklama1
                                        </p>
</div>
                                    <div class="acc-item">
                                        <div class="acc-toggle flex-middle">
                                            @a.HakkimdaBaslik1
                                            <span class="down"><i class="fas fa-plus"></i></span>
                                            <span class="up"><i class="fas fa-minus"></i></span>
                                        </div>
                                        <div class="acc-content">
                                            @a.HakkimdaAciklama2
                                         </div>
                                    </div>
                                    
                                    
                                </div>
                            </div>
                        </div>
                       }
                    </div>
                </div>
            </section>
            @foreach (var a in Model)
            {
                
            
            <section class="circle-side" style="background-image:url(images/bg/bg2-process.jpg)">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-3 col-lg-6 col-md-6 col-sm-12 text-center mb-5 mb-xl-0">
                            <div class="circle-progress tbottom text-light" data-color="#fff" data-height="2" data-size="165" data-processed="true">
                                <div class="inner-bar" data-percent="@a.Yuzde1Oran"><span><span class="percent">0</span>%</span></div>
                                <h4>@a.Yuzde1Baslik</h4>
                                <p class="main-color-stext">@a.Yuzde1Aciklama</p>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-6 col-md-6 col-sm-12 text-center mb-5 mb-xl-0">
                            <div class="circle-progress tbottom text-light" data-color="#fff" data-height="2" data-size="165" data-processed="true">
                                <div class="inner-bar" data-percent="@a.Yuzde2Oran"><span><span class="percent">0</span>%</span></div>
                                <h4>@a.Yuzde2Baslik</h4>
                                <p class="main-color-stext">@a.Yuzde2Aciklama</p>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-6 col-md-6 col-sm-12 text-center mb-5 mb-md-0">
                            <div class="circle-progress tbottom text-light" data-color="#fff" data-height="2" data-size="165" data-processed="true">
                                <div class="inner-bar" data-percent="@a.Yuzde3Oran"><span><span class="percent">0</span>%</span></div>
                                <h4>@a.Yuzde3Baslik</h4>
                                <p class="main-color-stext">@a.Yuzde3Aciklama</p>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-6 col-md-6 col-sm-12 text-center">
                            <div class="circle-progress tbottom text-light" data-color="#fff" data-height="2" data-size="165" data-processed="true">
                                <div class="inner-bar" data-percent="@a.Yuzde4Oran"><span><span class="percent">0</span>%</span></div>
                                <h4>@a.Yde4Baslik</h4>
                                <p class="main-color-stext">@a.Yuzde4Aciklama</p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            }

            <section id="portfolio-side" class="portfolio-side">
                <div class="container-fluid p-0">
                    <div class="project-filter-wrapper">
                       
                            
                        
                        <ul class="project_filters s-light">

                            <li><a href="#" data-filter="*" class="selected btn-details">HEPSİ<span class="filter-count"></span></a></li>
                             @foreach (var b in hizmetKategoris)
                            {
                                
                            <li><a class="btn-details" href="#" data-filter=".@b.KategoriID">@b.KategoriAd<span class="filter-count"></span></a></li>
                            }
                        </ul>
                        
                        <div class="projects-grid pf_4_cols style-1 img-scale w-auto no-gaps mx-0">
                            
                            <div class="grid-sizer"></div>
                            @foreach (var a in referanslar.Take(8))
                            {
                                
                           
                            <div class="project-item @a.KategoriID ">
                                
                                        
                                    
                                <div class="projects-box">
                                    <div class="projects-thumbnail">
                                        
                                            <img  src="@a.ReferansGörsel" alt="">
                                        
                                        <div class="overlay">
                                            <h5>@a.ReferansAd</h5>
                                            <i class="ot-flaticon-add"></i>
                                        </div>
                                    </div>
                                    <div class="portfolio-info">
                                        <div class="portfolio-info-inner">
                                            <h5>Stylish Family Appartment</h5>
                                            <p class="portfolio-cates">[@a.HizmetKategori.KategoriAd] </p>
                                        </div>
                                        
                                    </div>
                                </div>
                                
                            </div>
                            } 
                        </div>

                        <div class="space-70"></div>
                        <div class="space-5"></div>
                        <div class="ot-button text-center">
                            <a href="@Url.Action("ReferansFull","Anasayfa")" class="octf-btn octf-btn-light border-hover-light">Bütün projeler</a>
                        </div>
                    </div>
                </div>
            </section>
              <section class="process-step" style="background-image:url(images/bg/bg2-process.jpg)">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 text-center theratio-align-center">
                            @foreach (var a in Model)
                            {
                                
                           
                            <div class="ot-heading is-dots">
                                <span>[ @a.SurecParantezBaslik ]</span>
                                <h2 class="main-heading">@a.SurecBaslik</h2>
                                <br>
                                <p>@a.SurecAciklama</p>
                            </div>
                             }
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-md-12">
                            @foreach (var a in Model)
                            {
                                
                            
                            <div class="ot-process" data-tab="true">
                                <ul class="process_nav unstyle">
                                    <li class="current">
                                        <div class="icon-main">
                                            <span class="dcell"><i class="ot-flaticon-tip"></i></span>
                                            <span class="number-stroke">01</span>
                                        </div>
                                        <h5>@a.Asama1Baslik</h5>
                                    </li>
                                    <li>
                                        <div class="icon-main">
                                            <span class="dcell"><i class="ot-flaticon-money-back"></i></span>
                                            <span class="number-stroke">02</span>
                                        </div>
                                        <h5>@a.Asama2Baslik</h5>
                                    </li>
                                    <li>
                                        <div class="icon-main">
                                            <span class="dcell"><i class="ot-flaticon-3d-1"></i></span>
                                            <span class="number-stroke">03</span>
                                        </div>
                                        <h5>@a.Asama3Baslik</h5>
                                    </li>
                                    <li>
                                        <div class="icon-main">
                                            <span class="dcell"><i class="ot-flaticon-paint-1"></i></span>
                                            <span class="number-stroke">04</span>
                                        </div>
                                        <h5>@a.Asama4Baslik</h5>
                                    </li>
                                </ul>
                                <div class="process-des">
                                    <div class="process-des-item"> @a.Asama1Aciklama</div>
                                    <div class="process-des-item">@a.Asama2Aciklama </div>
                                    <div class="process-des-item">@a.Asama3Aciklama </div>
                                    <div class="process-des-item">@a.Asama4Aciklama </div>
                                </div>
                            </div>
                              }
                        </div>
                        
                    </div>
                </div>
            </section>

            <div class="referans"></div>

            <section class="benefit-side">
              
            </section>

       
            <div class="p-0">
                <img src="images/bg1-home6.jpg" alt="">
            </div>


            <section class="testi-side-title p-0">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 text-center theratio-align-center">
                            <div class="testi-heading-side">
                                <div class="ot-heading is-dots">
                                    <span>[ Müşteri Yorumlarımız ]</span>
                                    <h2 class="main-heading">Müşteri yorumlarımız</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <section class="testi-side pt-0">
                <div class="container">
                    <div class="ot-testimonials s-dark">
                        <div class="testimonial-inner ot-testimonials-slider-s1 owl-theme owl-carousel">
                            @foreach (var item in yorums)
                            {
                                
                           
                            <div class="testi-item">
                                <div class="ttext">
                                    <div class="layer-behind"></div>
                                    <span></span>
                                    "@item.YorumAciklama"                          
                                </div>
                                <div class="t-head flex-middle">
                                    <img src="images/testi2.png" alt="">
                                    <div class="tinfo">
                                        <h6>@item.YorumAd</h6>
                                        <span>@item.YorumSirket</span>
                                    </div>
                                </div>
                            </div>
                             }
                        </div>
                    </div>
                </div>
            </section>

          
            <div class="side-map">
                <div class="map">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d25175.38141754952!2d40.15266055000001!3d37.9322368!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14c3918527677761%3A0xf3f53a15466e8db4!2zVW5jYWzEsSwgUG9ydGFrYWwgQ2QgOSBBQSwgMDcwNzAgQmFodMSxbMSxIEvDtnnDvCBLw7Z5w7wvS29ueWFhbHTEsS9BbnRhbHlh!5e0!3m2!1str!2str!4v1663596892582!5m2!1str!2str" height="522" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
                </div>
            </div>

        </div>
        
 @foreach (var item in iletisims)
                    {
    <footer id="site-footer" class="site-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-sm-12 mb-4 mb-xl-0">
                    <div class="widget-footer footer-side-nav">
                        <img src="images/logo-footer.svg" class="footer-logo" alt="">
                        <p>We are team based on OceanThemes. Our expertise on Interior Design. Sed ut perspiciatis unde omnis iste natus error sit voluptatem accusantium doloremque laudantium, totam rem aperiam, eaque ipsa quae ab illo inventore veritatis et quasi architecto beatae vitae dicta sunt explicabo.</p>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12 text-right">
                    <div class="widget-footer footer-side-nav">
                        <h6>Contacts</h6>
                        <ul class="footer-list">
                            <li class="footer-list-item">
                                <span class="list-item-icon"><i class="ot-flaticon-place"></i></span>
                                <span class="list-item-text">@item.Adres</span>
                            </li>
                            <li class="footer-list-item">
                                <span class="list-item-icon"><i class="ot-flaticon-mail"></i></span>
                                <span class="list-item-text">@item.Mail</span>
                            </li>
                            <li class="footer-list-item">
                                <span class="list-item-icon"><i class="ot-flaticon-phone-call"></i></span>
                                <span class="list-item-text">@item.Telefon</span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer><!-- #site-footer -->
 }
</div><!-- #page -->
 
<a id="back-to-top" href="#" class="show"><i class="ot-flaticon-left-arrow"></i></a>
        <!-- jQuery -->
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/mousewheel.min.js"></script>
    <script src="~/js/lightgallery-all.min.js"></script>
    <script src="~/js/jquery.magnific-popup.min.js"></script>
    <script src="~/js/jquery.isotope.min.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/easypiechart.min.js"></script>
    <script src="~/js/jquery.countdown.min.js"></script>
    <script src="~/js/vegas.js"></script>
    <script src="~/js/scripts.js"></script>
    <script src="~/js/royal_preloader.min.js"></script>
    <!-- REVOLUTION JS FILES -->

    <script>
        window.jQuery = window.$ = jQuery;  
        (function($) { "use strict";
            //Preloader
            Royal_Preloader.config({
                mode           :   'progress',
                background     :   '#1a1a1a',
            });
        })(jQuery);
    </script>  
</body>
</html>